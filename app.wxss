/**app.wxss**/
@import 'weui.wxss';
page{
    background-color: #F5F5F5;
    overflow: hidden;
}
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    overflow: hidden;
}
.col_g{
    color: #56a41f;
}
.f_wei{
    font-weight: 700;
}
.ds_n{
    display: none;
}
.ds_b{
    display: block;
}
.bg_c{
    background-color: #EFA719!important;
}
.col_56{
    color: #56a41f!important;
}
/* 评价星级 */
.comment1-description{
  display:flex;
  flex-direction:row;
  margin-left: 25px;
}
.stars_cc{/* 灰色 */
    background-image:url('data:image/png;base64,');
    width: 50rpx;
    height: 50rpx;
    background-size: 50rpx 50rpx;
    margin-left: 30rpx;
    transition: 600ms;
}
.on_cc{/* 正常 */
    background-image:url('data:image/png;base64,');
    width: 50rpx;
    height: 50rpx;
    background-size: 50rpx 50rpx;
    transition: 1s;
    transform: scale(1,1)
}
.weui-cells_after-title{
  width: 92%;
  margin:0 auto;
  border:1px solid #ddd;
}

.btnSf{
  width: 92%;
  margin:30rpx auto;
  
}
.btnSf button{
    background: #57B6BA;
    color: #fff;
}

.btnSf button:active{
  background: #5BC2C6;
  color: #fff;
}
.assess{
    width:94%;
    padding: 27rpx 23rpx;
    background-color: #fff;
}
.assess_name{
    font-size: 24rpx;
    color: #222222;
    font-weight: 700;
}
.assess_xx{
    display: flex;
    flex-direction: row;
    font-size: 24rpx;
}
.xx_name{
    margin-top: 11rpx;
}
.environment_xx{
    display: flex;
    flex-direction: row;
    margin-top: 26rpx;
    font-size: 24rpx;
}
.environment_name{
    margin-top: 11rpx;
    margin-right: 24rpx;
}
/* 模板 */

.game_name {
    width: 85%;
}

.ball_xx {
    width: 94%;
    margin-top: 18rpx;
    background-color: #fff;
    padding: 30rpx 23rpx;
    font-size: 24rpx;
    border-bottom: 1px solid #ddd;
}

.xx_title {
    color: #222;
}

.xx_id {
    margin-top: 25rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.id_name {
    width: 30%;
    text-align: left;
}

.dz_name {
}

.img_show {
    padding: 27rpx 0;
    padding-bottom: 0;
}

.img_show>image {
    width: 217rpx;
    height: 154rpx;
    border: 1px solid #ddd;
}

.img_show>image:nth-child(2) {
    margin: 0 23rpx;
}

.ball_join {
    width: 94%;
    margin-top: 19rpx;
    background-color: #fff;
    padding: 27rpx 21rpx;
    font-size: 24rpx;
    border-bottom: 1px solid #ddd;
}

.join_title {
    width: 105%;
    display: flex;
    flex-direction: row;
}

.join_peo {
    padding-top: 5rpx;
}

.wx_peo {
    display: flex;
    flex-direction: row;
}

.peo_xx {
    text-align: center;
    margin-top: 25rpx;
}

.peo_xx>image {
    width: 125rpx;
    height: 97rpx;
}

.ball_bottom {
    width: 94%;
    margin-top: 18rpx;
    background-color: #fff;
    font-size: 24rpx;
    padding: 30rpx 23rpx;
    border-bottom: 1px solid #ddd;
}
.ball_require{
    display: flex;
    flex-direction: row;
    margin-top: 29rpx;
}
.require{
    text-align: center;
    border-right: 1px solid #ddd;
    width: 30%;
}
.require>view{
    margin-top: 25rpx;
}
.no_bor{
    border: none;
}
.registration_time{
    width: 94%;
    margin-top: 36rpx;
    margin-bottom: 24rpx;
    font-size: 20rpx;
    color: #424242;
}
.remark{
    width: 94%;
    font-size: 20rpx;
    color: #424242;
}
/* 评价星级图片 */
.pj_img{
    display: flex;
    flex-direction: row;
    margin-top: 29rpx;
}
.pj_img image{
    width: 125rpx;
	height: 97rpx;
    margin-right: 21rpx;
}
.pj_name{
    width: 112rpx;
    font-size: 22rpx;
    color: #222222;
    margin-top: 15rpx;
    text-align: center;
}
.checkbox_sty{
    width: 125rpx;
    text-align: center;
    font-size: 20rpx;
    margin-right: 53rpx;
}
.checkbox_sty>checkbox{
    zoom: 70%;
}
.pj_check .checkbox_sty:first-child{
    margin-left: 10px;
}
.peo_list{
    float: left;
}
.ball_name{
    display: flew;
    flex-direction: row;
    width: 94%;
    background-color: #fff;
    padding:30rpx 23rpx;
   
}