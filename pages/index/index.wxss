/**index.wxss**/

page {
    background-color: #f5f5f5;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200px;
}

/* 搜索 */

.page__bd {
    width: 100%;
    position: fixed;
    z-index: 9999;
}

.searchbar-result {
    margin-top: 0;
    font-size: 14px;
}

.searchbar-result:before {
    display: none;
}

.weui-cell {
    padding: 12px 15px 12px 35px;
}

/* banner */

.index_banner {
    width: 100%;
    height: 186rpx;
    margin-top: 97rpx;
}

.index_banner>image {
    width: 100%;
    height: 186rpx;
}

/* 三栏导航 */

.children_show {
    display: block;
}

.children_hide {
    display: none;
}

.index_search {
    width: 94%;
    background-color: #f5f5f5;
    margin-left:-46rpx;
}

.nav {
    width: 106%;
    position: relative;
    z-index: 99;
    display: flex;
    padding: 15rpx 0 15rpx;
}

.nav-son {
    display: flex;
    flex: 1;
    text-align: center;
    height: 40px;
    align-items: center;
    justify-content: center;
    font-size: 14px;
}

/* .borders{
    border-left: 1px solid #e6e6e6;
    border-right: 1px solid #e6e6e6;
} */

.content {
    display: inline-block;
    font-size: 28rpx;
    color: #222;
}

.icon {
    display: inline-block;
    border: 4px solid transparent;
    border-top: 4px solid #424242;
    margin-left: 5px;
    margin-top: 4px;
}

.temp {
    position: relative;
    z-index: 4;
    width: 103.5%!important;
}

.temp1 {
    display: none;
     width: 100%;
    max-height: 550rpx;
    overflow-y: scroll;
    padding: 0 0 0 20rpx;
    line-height: 100rpx;
    background: #fff;
}

.temp1>view:last-child {
    border-bottom: none !important;
}

.temp1>view {
    border-bottom: none !important;
}

.temp2 {
    display: none;
    width: 100%;
    max-height: 750rpx;
    overflow-y: auto;
    overflow: hidden;
    padding: 0 0 0 20rpx;
    line-height: 100rpx;
    background: #fff;
}

/* .slidedown {
    transform: translateY(0%);
} */

.temp view {
    border-bottom: 1px solid #d1d3d4;
    font-size: 28rpx;
    color: #222;
}
.px_flex{
    display: flex;
    flex-direction: row;
    justify-content:space-between;
}
.px_flex >view{
    border-bottom: none;
}
.px_flex icon{
    margin-right: 90rpx!important;
}
.quyu {
    border-top: 1px solid #e4e4e4;
    position: relative;
    display: none;
    height: 450rpx;
    z-index: 5;
    width: 106.5%;
}

.quyu .qy {
    overflow-y: scroll;
    float: left;
    height: 450rpx;
    line-height: 80rpx;
    box-sizing: border-box;
    font-size: 14px;
    color: #717273;
}

.qy view {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    padding-left: 15rpx;
    color: #222;
}

.qy::-webkit-scrollbar {
    display: none;
}

.quyu-left {
    background-color: #fff;
    width: 20%;
}

.quyu-center {
    background-color: #f5f5f5;
    width: 25%;
}

.quyu-center>view {
    text-align: center;
}

.quyu-right {
    background: #fff;
    width: 55%;
}

.quyu-right>view {
    border-bottom: 1px solid #e4e4e4;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.con_sty{
    line-height:2rpx;
    margin-right:20rpx;
}
.current {
    background-color: #fefefe;
    color: #56a41f !important;
}

.current2 {
    color: #56a41f !important;
    background-color: #fff !important;
}

.fullbg {
    position: fixed;
    z-index: 1;
    background: rgb(1, 1, 1);
    transition: all 2s;
    opacity: 0;
    display: none;
    top: 50%;
    left: 0;
    overflow-x: hidden;
}

.fullopacity {
    opacity: 0.5;
    display: block;
}

.nav-son.active .content {
    color: #56a41f;
}

.nav-son.active .icon {
    border-bottom: 4px solid #61beff;
    border-top: 0;
}

@keyframes slidown {
    from {
        transform: translateY(-100%);
    }

    to {
        transform: translateY(0%);
    }
}

.slidown {
    display: block;
    /* animation: slidown .7s ease-in both; */
}

@keyframes slidup {
    from {
        transform: translateY(0%);
    }

    to {
        transform: translateY(-100%);
    }
}

.slidup {
    display: block;
    animation: slidup 0.7s ease-in both;
}

.disappear {
    display: none;
}

